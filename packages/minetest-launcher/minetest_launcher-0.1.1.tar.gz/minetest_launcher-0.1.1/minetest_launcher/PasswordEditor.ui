<interface>
    <template class="PasswordEditor" parent="GtkWindow">
        <child>
            <object class="GtkBox">
                <property name="orientation">GTK_ORIENTATION_VERTICAL</property>
                <child>
                    <object class="GtkEntry" id="username"></object>
                </child>
                <child>
                    <object class="GtkPasswordEntry" id="password"></object>
                </child>
            </object>
        </child>
    </template>
</interface>
